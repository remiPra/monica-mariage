---
import Header from "../components/Header.astro";
import '../styles/global.css';
import FolatingButtonWhatsapp from "../components/FolatingButtonWhatsapp.astro";
import Footer from "../components/Footer.astro";
import AppointmentBar from "../components/AppointmentBar.astro";
import ImagePreloader from "../components/ImagePreloader.astro";
import HeroSection from "../components/HeroSection.astro";
import { sanityClient } from "../sanityClient";
import { optimizeCloudinaryUrl } from "../lib/optimizeCloudinaryUrl";
import FavoriButton from "../components/FavoriButton.astro";

// --- Variables et Props ---
const title = "Robe Mariée Sur-Mesure Toulouse | Créations Uniques | Monica Mariage";
const description = "Robe de mariée sur-mesure à Toulouse : essayage privé, créations ajustées à votre morphologie (34–44), délais 3–4 mois  et paiement en 3 fois.";
const mainImageUrl = "";
const analyticsId = "G-H8RDXC45EX";

const baseUrl = "https://www.monicamariage.com";
const pagePath = "/robe-mariee-sur-mesure-toulouse";
const pageUrl = `${baseUrl}${pagePath}`;

// --- Logique de récupération des données ---
const robesData = await sanityClient.fetch(`       
  *[_type == "robe" && category == "robes-de-mariee"]
  | order(order asc){
    _id,
    dressName,
    category,
    sousCategorie,
    "slug": slug.current,
    order,
    imagesCloudinary[]{ url, alt }
  }
`);

const robes = (robesData || []).map((robe) => ({
  ...robe,
  images: (robe.imagesCloudinary || []).map((img) => ({
    alt: img.alt || robe.dressName,
    optimizedImages: {
      gallery: {
        desktop: optimizeCloudinaryUrl(img.url, false),
        tablet: optimizeCloudinaryUrl(img.url, false),
        mobile: optimizeCloudinaryUrl(img.url, "imageMobileGallery"),
      },
    },
  })),
}));

const preloadImageUrls = robes
  .slice(0, 4)
  .map((robe) => robe.images[0]?.optimizedImages.gallery.mobile)
  .filter(Boolean);

// --- Préparation des Données Structurées (Schema.org) ---

const itemListElements = robes.map((robe, index) => ({
  "@type": "ListItem",
  "position": index + 1,
  "item": {
    "@type": "Product",
    "name": robe.dressName ? robe.dressName.charAt(0).toUpperCase() + robe.dressName.slice(1).toLowerCase() : "Robe de mariée",
    "url": `${baseUrl}/robes-de-mariee/${robe.sousCategorie || 'collection'}/${robe.slug}`,
    "image": robe.images[0]?.optimizedImages.gallery.mobile,
    "description": `Découvrez la robe de mariée ${robe.dressName}, une création unique disponible pour essayage chez Monica Mariage à Toulouse.`,
    "brand": {
      "@type": "Brand",
      "name": "Monica Mariage"
    },
    "offers": {
      "@type": "Offer",
      "availability": "https://schema.org/InStock",
      "priceCurrency": "EUR",
      "price": "1200", 
      "lowPrice": "750", 
      "highPrice": "1850", 
      

    }
  }
}));

const schemaData = {
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "CollectionPage",
      "name": title,
      "url": Astro.url.href,
      "description": description,
      "mainEntity": {
        "@type": "ItemList",
        "numberOfItems": robes.length,
        "itemListElement": itemListElements
      }
    },
    {
      "@type": "BreadcrumbList",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Accueil",
          "item": baseUrl + "/"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "Robe de mariée sur-mesure à Toulouse",
          "item": pageUrl
        }
      ]
    }
  ]
};
